{
  "version": "0.0.0",
  "name": "dpaste-js",
  "umd:name": "dpaste-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MRDGH2821/dpaste-js.git"
  },
  "homepage": "https://github.com/MRDGH2821/dpaste-js",
  "description": "MRDGH2821/dpaste-js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "MRDGH2821",
    "url": "https://bit.ly/mrdgh2821"
  },
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "uvu -r ts-node/register test",
    "mytest": "node test.js ",
    "prepublish": "npm run build",
    "premytest": "yalc publish && yalc link dpaste-js",
    "tsc-init": " cd node_modules/.bin/ && tsc --init"
  },
  "bugs": {
    "url": "https://github.com/MRDGH2821/dpaste-js/issues"
  },
  "keywords": [
    "dpaste",
    "pastebin",
    "wrapper"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "8.1.0",
    "rollup": "2.21.0",
    "rollup-plugin-terser": "6.1.0",
    "rollup-plugin-typescript2": "0.27.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.6",
    "uvu": "0.0.19",
    "yalc": "^1.0.0-pre.53"
  },
  "dependencies": {
    "axios": "^0.24.0"
  }
}
