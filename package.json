{
  "name": "dpaste-ts",
  "umd:name": "dpaste-ts",
  "version": "1.0.4",
  "description": "Nodejs wrapper for dpaste.com written using Typescript",
  "keywords": [
    "dpaste",
    "pastebin",
    "wrapper"
  ],
  "homepage": "https://github.com/MRDGH2821/dpaste-js#readme",
  "bugs": {
    "url": "https://github.com/MRDGH2821/dpaste-js/issues"
  },
  "license": "MIT",
  "author": {
    "name": "MRDGH2821",
    "url": "https://bit.ly/mrdgh2821"
  },
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/mrdgh2821"
  },
  "files": [
    "dist",
    "types"
  ],
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MRDGH2821/dpaste-js.git"
  },
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "uvu -r ts-node/register test",
    "mytest": "node test.js ",
    "tsc-init": " cd node_modules/.bin/ && tsc --init",
    "lint": "npx eslint . --fix --ignore-path .gitignore ",
    "lint:mega": "npx mega-linter-runner@latest --flavor javascript",
    "pretty": "npx prettier . --write --ignore-path .gitignore"
  },
  "dependencies": {
    "axios": "^0.25.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "prettier-config-mrdgh2821": "^3.0.0",
    "rollup": "^2.66.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "uvu": "^0.5.3"
  }
}
